PROGRAM PROGRAM 1
IDENT circle 1
REAL REAL 3
DCOLON :: 3
IDENT r 3
COMMA , 3
IDENT a 3
COMMA , 3
IDENT p 3
COMMA , 3
IDENT b 3
PRINT PRINT 4
DEF * 4
COMMA , 4
SCONST 44 "Please enter the two radii values of circles" 3
IF IF 5
LPAREN ( 5
IDENT r 5
EQ == 5
ICONST 5 5
RPAREN ) 5
IDENT a 6
ASSOP = 6
LPAREN ( 6
RCONST 3.14 6
RPAREN ) 6
MULT * 6
IDENT r 6
MULT * 6
IDENT r 6
END END 7
IF IF 7
PRINT PRINT 8
DEF * 8
COMMA , 8
SCONST 23 "The output results are " 7
COMMA , 8
IDENT a 8
COMMA , 8
IDENT p 8
COMMA , 8
IDENT b 8
END END 9
PROGRAM PROGRAM 9
IDENT circle 9
